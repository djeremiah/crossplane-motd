#!/bin/sh
red=$(tput setaf 210)
yellow=$(tput setaf 220)
blue=$(tput setaf 043)
reset=$(tput sgr0)                                     
                                                                             
  
echo "$red
  ____                         _                  
 / ___|_ __ ___  ___ ___ _ __ | | __ _ _ __   ___ $yellow
| |   | '__/ _ \\/ __/ __| '_ \\| |/ _\` | '_ \ / _ \\
| |___| | | (_) \\__ \\__ \\ |_) | | (_| | | | |  __/$blue
 \\____|_|  \\___/|___/___/ .__/|_|\__,_|_| |_|\\___|
                        |_|$reset
"